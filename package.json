{
  "name": "giantdb",
  "version": "3.1.0",
  "description": "Large object database in native JavaScript, with encryption support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore .",
    "lint-fix": "tsc --noEmit && eslint --fix --ignore-path .gitignore .",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\"",
    "coverage": "c8 --reporter=text --reporter=lcov --all --src=src npm test",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/giantdb.git"
  },
  "keywords": [
    "object",
    "storage",
    "store",
    "database",
    "file",
    "encryption",
    "large",
    "native"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/giantdb/issues"
  },
  "homepage": "https://github.com/meyfa/giantdb",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@meyfa/eslint-config": "2.0.0",
    "@types/chai": "4.3.1",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.45",
    "c8": "7.11.3",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "delay": "5.0.0",
    "eslint": "8.20.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "mocha": "10.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "fs-adapters": "^6.0.1",
    "writable-wrapper": "^3.1.0"
  }
}
