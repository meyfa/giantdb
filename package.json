{
  "name": "giantdb",
  "version": "2.0.1",
  "description": "Large object database in native JavaScript, with encryption support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ignore-path .gitignore .",
    "test": "npm run clean && ts-mocha --recursive \"test/**/*.test.*\"",
    "coverage": "nyc --reporter=lcov --all --include \"lib/**/*.ts\" npm test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/giantdb.git"
  },
  "keywords": [
    "object",
    "storage",
    "store",
    "database",
    "file",
    "encryption",
    "large",
    "native"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/giantdb/issues"
  },
  "homepage": "https://github.com/meyfa/giantdb",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "delay": "^5.0.0",
    "eslint": "^7.10.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "fs-adapters": "^6.0.1",
    "writable-wrapper": "^3.0.0"
  }
}
